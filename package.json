{
  "name": "@nemoventures/adonis-clickhouse",
  "description": "ClickHouse client for AdonisJS",
  "version": "0.1.0",
  "engines": {
    "node": ">=20.6.0"
  },
  "main": "build/index.js",
  "type": "module",
  "files": [
    "build",
    "!build/bin",
    "!build/tests"
  ],
  "exports": {
    ".": "./build/index.js",
    "./types": "./build/src/types.js",
    "./services/main": "./build/services/main.js",
    "./clickhouse_provider": "./build/providers/clickhouse_provider.js"
  },
  "scripts": {
    "clean": "del-cli build",
    "copy:templates": "copyfiles --up 1 \"stubs/**/*.stub\" build",
    "typecheck": "tsc --noEmit",
    "index:commands": "adonis-kit index build/commands",
    "lint": "eslint",
    "format": "prettier --write .",
    "quick:test": "node --enable-source-maps --import=ts-node-maintained/register/esm bin/test.ts",
    "pretest": "npm run lint",
    "test": "c8 npm run quick:test",
    "prebuild": "npm run lint && npm run clean",
    "build": "tsup-node --metafile && tsc --emitDeclarationOnly --declaration",
    "postbuild": "npm run copy:templates && npm run index:commands",
    "version": "npm run build",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@adonisjs/assembler": "^7.8.2",
    "@adonisjs/core": "^6.17.2",
    "@adonisjs/eslint-config": "^2.0.0",
    "@adonisjs/prettier-config": "^1.4.0",
    "@adonisjs/tsconfig": "^1.4.0",
    "@japa/assert": "^4.0.1",
    "@japa/file-system": "^2.3.2",
    "@japa/runner": "^4.2.0",
    "@swc/core": "^1.10.16",
    "@types/node": "^22.13.4",
    "c8": "^10.1.3",
    "copyfiles": "^2.4.1",
    "del-cli": "^6.0.0",
    "eslint": "^9.20.1",
    "np": "^10.0.6",
    "prettier": "^3.5.1",
    "prettier-edgejs": "^0.2.35",
    "ts-node-maintained": "^10.9.5",
    "tsup": "^8.3.6",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "@clickhouse/client": "^1.10.1",
    "@poppinss/utils": "^6.9.2"
  },
  "license": "MIT",
  "repository": "https://github.com/nemoengineering/adonis-clickhouse",
  "publishConfig": {
    "provenance": true,
    "access": "public"
  },
  "prettier": "@adonisjs/prettier-config",
  "c8": {
    "reporter": [
      "text",
      "html"
    ],
    "exclude": [
      "tests/**"
    ]
  },
  "tsup": {
    "entry": [
      "index.ts",
      "src/types.ts",
      "providers/clickhouse_provider.ts",
      "services/main.ts"
    ],
    "outDir": "./build",
    "clean": true,
    "format": "esm",
    "dts": false,
    "sourcemap": true,
    "target": "esnext"
  }
}
